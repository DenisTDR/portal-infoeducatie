@using AutoMapper
@using InfoEducatie.Main.Pages
@using MCMS.Base.Data
@inject IRepository<PageEntity> PagesRepo
@inject IMapper Mapper
@{
    var homePage = await PagesRepo.GetOneOrThrow("home");
    var homePageVm = Mapper.Map<PageViewModel>(homePage);
}

<partial name="../Pages/Details" model="homePageVm"/>
